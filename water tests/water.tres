// FloatOp:20
	float n_out20p0 = n_out21p0 + n_out24p0;


// FloatOp:25
	float n_out25p0 = n_out20p0 * n_out4p0;


// FloatFunc:26
	float n_out26p0 = abs(n_out25p0);


// VectorOp:5
	vec3 n_out5p0 = vec3(n_out2p0.xyz) + vec3(n_out26p0);


// FloatParameter:23
	float n_out23p0 = Alpha;


// Output:0
	ALBEDO = n_out5p0;
	ALPHA = n_out23p0;


}
